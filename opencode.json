      ".envrc": "deny",
      "secrets/**": "deny",
      "config/credentials.json": "deny",
      "mise.local.toml": "deny",
      ".direnv/**": "deny",
      "devenv.local.nix": "deny",
      "build": "deny"
    },
    "edit": {
