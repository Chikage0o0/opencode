  "permission": {
    "bash": {
      "*": "ask",
      "grep *": "allow",
      "rg *": "allow",
      "find *": "allow",
      "ls *": "allow",
      "git status*": "allow",
      "git diff*": "allow",
      "git log*": "allow",
      "npm *": "allow",
      "rm *": "ask",
      "git ls-files*": "allow",
      "rm commit.msg": "allow",
      "del commit.msg": "allow",
      "cargo check*": "allow",
      "ls": "allow",
      "dir": "allow",
      "type *": "allow",
      "whoami": "allow",
      "pwd": "allow"
    },
    "read": {
